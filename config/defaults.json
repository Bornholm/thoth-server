{

	"database": "mongodb://localhost/thoth",

	"server": {
		"address": "localhost",
		"port": 3000,
		"serveClient": true,
		"ssl": {
			"key": null,
			"cert": null,
			"pfx": null
		}
	},

	"plugins": {
		"thoth-local-auth": {
			"path": "./plugins/thoth-local-auth",
			"options": {
				"rounds": 10
			}
		}
	},

	"auth": "local",

	"encryption": {
		"algorithm": "aes256",
		"hmacPath": ".hmac"
	}
	
}